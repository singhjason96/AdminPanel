<link href="https://fonts.googleapis.com/css?family=Alegreya+SC|Rasa" rel="stylesheet">
<div class="background">
</div>
<div class="card">
<h1>New Cohort</h1>
<div class="container">
<%= form_for @cohort, url: {action: "create"}, html: {class: "nifty_form"} do |f| %>
    <div class="name">
    <%= f.label :name%><br>
    <%= f.text_field :name %>
    </div>
    <div class= "start">
    <%= f.label :start_date%><br>
    <%= f.date_field :start_date %>
    </div>
    <div class="end">
    <%= f.label :end_date%><br>
    <%= f.date_field :end_date %>
    </div>
    
   

<div class="submit">
    <%= f.submit "Create" %>
    </div>
  <% end %>
</div>
</div>